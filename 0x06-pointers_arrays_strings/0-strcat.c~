#include "holberton.h"

/**
 * _strcat - concatenates two strings.
 *@src: first parameter in _strcat.
 *@dest: second parameter in _strcat.
 * Return: *dest.
 */
char *_strcat(char *dest, char *src)
{
char str1, str2, str3;
src = str1;
dest = str3;
while (*src != 0)
{
*dest = *src;
src++;
dest++;
}
src = str2;
while (*src != 0)
{
*dest = *src;
src++;
dest++;
}
*dest = 0;

return (*dest);
}
